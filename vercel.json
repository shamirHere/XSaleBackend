{
    "name": "your-project-name",
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/v1/user(.*)", "dest": "app.js" },
      { "src": "/api/v1/listing(.*)", "dest": "app.js" },
      { "src": "/(.*)", "dest": "app.js" }
    ]
  }
  
  